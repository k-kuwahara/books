{
  "name": "books",
  "version": "0.1.0",
  "scripts": {
    "init": "gitbook init",
    "start:js": "gitbook serve js",
    "start:riot": "gitbook serve riot",
    "build-js": "gitbook build js && rm -rf pages/js/ && cp -R js/_book/ pages/js/",
    "build-riot": "gitbook build riot && rm -rf pages/riot/ && cp -R riot/_book/ pages/riot/",
    "publish": "gh-pages -d pages -m 'auto publish'"
  },
  "author": "k-kuwahara",
  "license": "MIT",
  "devDependencies": {
    "gitbook-cli": "^2.3.2"
  },
  "dependencies": {
    "gh-pages": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/k-kuwahara/books.git"
  },
  "bugs": {
    "url": "https://github.com/k-kuwahara/books/issues"
  },
  "homepage": "https://github.com/k-kuwahara/books#readme",
  "description": ""
}
